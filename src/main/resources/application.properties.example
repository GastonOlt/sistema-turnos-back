# Archivo de ejemplo de configuración de Spring Boot

spring.datasource.url=jdbc:postgresql://localhost:5432/sistema_turnos
spring.datasource.username=USUARIO_DB
spring.datasource.password=CONTRASEÑA_DB

spring.datasource.driver-class-name=org.postgresql.Driver
spring.jpa.database-platform=org.hibernate.dialect.PostgreSQLDialect

spring.jpa.hibernate.ddl-auto=update

spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true

spring.servlet.multipart.max-file-size=10MB
spring.servlet.multipart.max-request-size=20MB

jwt.secret=CLAVE_SECRETA